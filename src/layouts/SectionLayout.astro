---
/**
 * Layout used by a specific section (e.g.: blog, contacts, etc.). Currently only a wrapper around content layout.
 */

import ContentLayout from "./ContentLayout.astro";

interface Props {
  pageTitle?: string,
  activePage?: string
}

const { pageTitle, activePage } = Astro.props;
---

<ContentLayout pageTitle={pageTitle} activePage={activePage}>
  <slot></slot>
</ContentLayout>
